{
  "welcomeFile": "/appadmin/index.html",
  "authenticationMethod": "route",
  "logout": {
    "logoutEndpoint": "/do/logout"
  },
  "routes": [
    {
      "source": "/admin/(.*)$",
      "target": "/admin/$1",
      "authenticationType": "xsuaa",
      "destination": "srv_api",
      "csrfProtection": true
    },    
    {
      "source": "^/appadmin/(.*)$",
      "target": "$1",
      "localDir": "webapp"
    }
  ]
}